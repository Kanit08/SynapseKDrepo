				"source": [
					"df1=df.withColumn('date',current_date())\n",
					"df1.show()\n",
					"df2=df1.withColumn('dateformat',date_format('date', 'MM/dd/yyy'))\n",
					"df2.show()\n",
					"df3=df2.withColumn('currdate',to_date(df2.dateformat,'MM/dd/yyyy'))\n",
